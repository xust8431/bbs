<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.xust.bbs.entity.User" table="user">
		<id name="id" type="string" column="user_id"></id>
		<property name="name" type="string" column="user_name"></property>
		<property name="password" type="string" column="user_password"></property>
		<property name="token" type="string" column="user_token"></property>
		<property name="nick" type="string" column="user_nick"></property>
		<property name="icon" type="string" column="user_icon"></property>
		<property name="email" type="string" column="user_email"></property>
		<property name="power" type="string" column="user_power"></property>
	</class>
</hibernate-mapping>